<div class="container" ng-controller="phoneNumberController">
	<div ng-repeat="entrant in entrants">
		<div class="entry text-center">
			<div class="row">
				<div class="col-md-3 name">
					<h2>{[{entrant.name}]}</h2>
				</div>
				<div class="col-md-6">
					<input type="text" class="form-control phoneNumber" id="exampleInputAmount" placeholder="Phone Number">
				</div>
			</div>
		</div>
	</div>
	<div class="col-md-4 col-md-offset-4">
	<button class="btn btn-warning btn-lg" ng-click="postNumbers()">
		Set up Phone Numbers
	</button>
	</div>
</div>
